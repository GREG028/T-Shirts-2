<div>
  <label>Title</label>
  <input type="text" name="title" value="<%= design.title %>">
</div>
<div>
    <label>Designer</label>
<select name="author">
<% designers.forEach(designer => { %>
  <% if (designer.id === design.author) { %>
    <option selected label="<%= designer.name %>" value="<%= designer.id%>">
  <% } else { %>
    <option label="<%= designer.name %>" value="<%= designer.id%>"></option>
    <% } %>
<% })%>
</select>
</div>
<div>
<label>Publish Date</label>
  <input type="date" name="publishDate" value="<%= design.publishDate == null ? '' : book.publishDate.toISOString().split('T')[0] %>">
</div>
<div>
    <label>File Size</label>
      <input type="text" name="fileSize" value="<%= design.fileSize %>">
</div>
<div>
  <label>Cover</label>
    <input type="file" name="cover">
</div>
<div>
  <label>Description</label>
    <textarea name="description"><%= design.description %></textarea>
</div>